{
  "name": "@figliolia/promises",
  "version": "1.0.1",
  "description": "A small promise-wrapping library providing Cancellable and Timed Promises",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src/*"
  ],
  "author": "Alex Figliolia",
  "license": "MIT",
  "homepage": "https://github.com/alexfigliolia/promises#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexfigliolia/promises.git"
  },
  "keywords": [
    "promises",
    "cancellable promise",
    "cancellable",
    "timed promise"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --env=jsdom --coverage --testResultsProcessor ./node_modules/jest-junit",
    "build": "npx ts-packager -e src",
    "lint": "tsc --noemit && eslint ./ --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@figliolia/ts-packager": "^1.0.3",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^4.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
